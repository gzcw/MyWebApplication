@{
    ViewBag.Title = "Index";
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @*<link href="~/areas/Workflow/Scripts/JQueryEasyUI/themes/green/easyui.css" rel="stylesheet" />*@
    <link href="~/areas/Workflow/Scripts/JQueryEasyUI/themes/default/easyui.css" rel="stylesheet" />
    <link href="~/areas/Workflow/Scripts/JQueryEasyUI/themes/icon.css" rel="stylesheet" />
    <link href="~/areas/Workflow/Css/main.css" rel="stylesheet" />
    <link href="~/areas/Workflow/Css/workflow.css" rel="stylesheet" />

    <script src="~/areas/Workflow/Scripts/JQuery/jquery-1.11.1.min.js"></script>
    <script src="~/areas/Workflow/Scripts/JQuery/jquery-migrate-1.2.1.min.js"></script>
    <script src="~/areas/Workflow/Scripts/JQueryEasyUI/jquery.easyui.min.js"></script>

    <script src="~/areas/Workflow/Scripts/JQueryEasyUI/locale/easyui-lang-zh_CN.js"></script>
    <script src="~/areas/Workflow/Scripts/Common/UserComponent.js"></script>
    <script src="~/areas/Workflow/Scripts/Common/PageCommon.js"></script>

    <script src="~/areas/Workflow/Scripts/Common/easyui-extension.js"></script>
    <script>
        window.RootDirectory = "@Request.ApplicationPath";
        Array.prototype.indexOf = Array.prototype.indexOf || function (searchElement, fromIndex) {
            if (this === undefined || this === null) {
                throw new TypeError('"this" is null or not defined');
            }
            var length = this.length >>> 0; // Hack to convert object.length to a UInt32
            fromIndex = +fromIndex || 0;
            if (Math.abs(fromIndex) === Infinity) {
                fromIndex = 0;
            }
            if (fromIndex < 0) {
                fromIndex += length;

                if (fromIndex < 0) {
                    fromIndex = 0;
                }
            }
            for (; fromIndex < length; fromIndex++) {
                if (this[fromIndex] === searchElement) {
                    return fromIndex;
                }
            }
            return -1;
        };
        window.console = window.console || { log: function () { } };
    </script>
</head>
<body>
    <div class="easyui-layout" data-options="fit:true,border:false">
        <div data-options="region:'north',border:false" style="height: 34px;">
            @RenderSection("searchform", required: false)
        </div>
        <div data-options="region:'center',border:false">
            @RenderBody()
        </div>
    </div>
</body>
</html>
