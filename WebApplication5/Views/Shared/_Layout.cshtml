<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link rel="icon" href="~/Content/images/logo.png" type="image/x-icon" />
    <script src="~/Scripts/jquery-1.11.0.min.js"></script>
    <script src="~/Scripts/jquery-migrate-1.2.1.min.js"></script>
    <script src="~/Scripts/jquery-easyui-1.7.5/jquery.easyui.min.js"></script>
    <script src="~/Scripts/ajaxfileupload.js"></script>
    <script src="~/Scripts/jquery-easyui-1.7.5/locale/easyui-lang-zh_CN.js"></script>
    @Scripts.Render("~/bundles/script")
    <link href="~/Scripts/jquery-easyui-1.7.5/themes/default/easyui.css" rel="stylesheet" />
    <link href="~/Scripts/jquery-easyui-1.7.5/themes/icon.css" rel="stylesheet" />
    @Styles.Render("~/Content/CSS/mycss")
    <script>
        window.applicationPath = "@Request.ApplicationPath";
        window.applicationPath = window.applicationPath == "/" ? "" : window.applicationPath;
        $(document).ready(function () {
            setTimeout(function () {
                initPermission();
            })
        });
    </script>
</head>
<body>
    @RenderBody()
</body>
</html>